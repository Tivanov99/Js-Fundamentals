function concatenate(number) {
    let numberAsString = number.toString();
    let sum = 0;
    for (let index = 0; index < numberAsString.length; index++) {
        sum += Number(numberAsString[index]);
    }
    let contains = 'False';
    let AsString = sum.toString();
    for (let i = 0; i < AsString.length; i++) {
        if (AsString[i] == '9') {
            contains='True';
            break;
        }
    }
        console.log(`${number} Amazing? ${contains}`)
}

function Solve(input) {
    class cat {
        constructor(name, age) {
            this.name = name;
            this.age = age;
        }
        Meow = function () {
            console.log(`${this.name}, age ${this.age} says Meow`);
        }
    }
    for (let element of input) {
        let splited= element.split(' ');
        let Cat= new cat(splited[0],splited[1]);
        Cat.Meow();
    }

}
